#include <stdio.h>

/**
 * main - entry point of program
 *
 * while loop - iterate through ASCII values for 0-9
 *
 * putchar - put numbers to std output
 *
 * Return: (0) - always - success
 */

int main(void)
{
	int i = 48;

	while (i < 58)
	{
		putchar(i);

		if (i != 57)
		{
			putchar(',');
			putchar(' ');
		}
		i++;
	}
	putchar('\n');
	return (0);
}
