#include <stdio.h>

/**
 * main - entry point of program
 *
 * for loop - iterate through base 10 numbers - using ASCII conversion
 *
 * putchar - prints characters to std out, and prints newline outside for loop
 *
 * Return: (0) - always - success
 *
 */

int main(void)
{
	int i = 48;

	while (i < 58)
	{
		putchar(i);
		i++;
	}

	putchar('\n');
	return (0);
}
