#include <unistd.h>
#include "holberton.h"
/**
 * _putchar - writes the character c to stdout
 * @c: The character to print
 *
 * Return: On success 1.
 * On error, -1 is returned, and errno is set appropriately.
 */

void print_alphabet(void)
{
	char ch[] = "Holberton";
	i = 0;

	while (i < 11)
	{
		_putchar(ch[i]);
		i++;
	}
	_putchar('\n');
}
