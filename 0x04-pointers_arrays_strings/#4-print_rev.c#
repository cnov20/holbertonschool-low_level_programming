#include "holberton.h"

/**
 * print_rev - prints string in reverse, followed by new line to std out
 * @s: string to be printed, with new line, to std out
 *
 * Return: (0)
 */

void print_rev(char *s)
{
	int end = _strlen[s] - 1;
        int i = end;

	while (i >= end)
        {
			_putchar(s);
			end++;
		}
		i--;
	}

        _putchar('\n');
}
