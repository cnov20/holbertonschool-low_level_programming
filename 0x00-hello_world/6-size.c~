#include <stdio.h>
/**
 * main - Entry point of program
 *
 * printf - Prints to standard output
 *
 * sizeof - Reads and outputs the size of each data type, in bytes
 *
 * Return: 0
 */

int main(void)
{
	char c;
	int i;
	long l;
	long long l_l;
	float f;

	printf("Size of a char: %lu byte(s)\n", (unsigned long)sizeof(c));
	printf("Size of an int: %lu byte(s)\n", (unsigned long)sizeof(i));
	printf("Size of a long int: %lu byte(s)\n", (unsigned long)sizeof(i));
	printf("Size of a long long int: %lu byte(s)\n", (unsigned long)sizeof(i));
	printf("Size of a float: %lu byte(s)\n", (unsigned long)sizeof(f));
	return (0);
}
